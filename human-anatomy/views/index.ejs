                <li><a href='/endocrine'>Endocrine</a></li>
                <li><a href='/integumentary'>Integumentary</a></li>
                <li><a href='/lymphatic'>Lymphatic</a></li>
                <li><a href='/urinary'>Urinary</a></li>
                <li><a href='/reproductive'>Reproductive</a></li>
                <li><a href='/eye'>Eye Anatomy</a></li>
                <li id="footer">
                    <footer>Eric Vargas &copy; 2019</footer>
                </li>
            </ul>
        </aside>
        <main id="main">
            <% data.forEach(function(d) { %>
            <div class="item" data-id=<%=d.id%>>
                <img src=<%=d.image %>>
                <br>
                <p><%= d.description %></p>
            </div>
            <% }) %>
        </main>
    </div>
</div>


<% include ./partials/end %>

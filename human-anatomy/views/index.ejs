<% include ./partials/boilerplate %>
<div class="container">
    <header><%= title %></header>
    <div class="page">
        <button id="btn">Systems</button>
        <aside id="aside" class="hide">
            <span id="navBtn">Systems</span>
            <ul>
                <li id="nav">
                    <nav>Systems</nav>
                </li>
                <li id="welcome"><a href='/'>Welcome</a></li>
                <li><a href='/cardiovascular'>Cardiovascular</a></li>
                <li><a href='/digestive'>Digestive</a></li>
                <li><a href='/respiratory'>Respiratory</a></li>
                <li><a href='/musculoskeletal'>Muscular</a></li>
                <li><a href='/skeletal'>Skeletal</a></li>
